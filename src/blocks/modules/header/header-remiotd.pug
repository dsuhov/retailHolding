              button.glide__arrow.glide__arrow--black.glide__arrow--left.hb-logos-controls__prev
                svg(viewbox="0 0 6 10" xmlns="http://www.w3.org/2000/svg")
                  path(d="M6 9.56128L0 5.03921L6 0.584473V9.56128Z")
              button.glide__arrow.glide__arrow--black.glide__arrow--right.hb-logos-controls__next
                svg(viewbox="0 0 6 10" xmlns="http://www.w3.org/2000/svg")
                  path(d="M6 9.56128L0 5.03921L6 0.584473V9.56128Z")
              .hb-logos__logos-media
                .hb-logos__logo
                  .image
                    picture.image__picture
